    "@nuxtjs/eslint-module": "1.0.0",
    "@nuxtjs/stylelint-module": "3.1.0",
    "@types/jest": "^29.2.3",
    "@vue/test-utils": "^1.3.6",
    "@vue/vue2-jest": "^29.2.6",
    "babel-eslint": "10.0.1",
    "babel-jest": "^29.6.1",
    "eslint": "6.1.0",
    "eslint-config-prettier": "6.10.0",
